    "def word_count(song_lyrics):\n",
    "    counts = dict()\n",
    "    words = song_lyrics.split()\n",
    "\n",
    "    for word in words:\n",
    "        if word in counts:\n",
    "            counts[word] += 1\n",
    "        else:\n",
    "            counts[word] = 1\n",
    "\n",
    "    return counts\n",
    "\n",
    "print( word_count(\"hey hey yo yo awesome\"))"
   ]
